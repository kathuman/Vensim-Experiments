{UTF-8}
"AT(t)"=
	1/(DUV*k)
	~	s
	~	Effective irradiance
	|

"dDuv/dt"=
	IF THEN ELSE( Time<te , I , 0 )
	~	
	~		|

DUV= INTEG (
	"dDuv/dt",
		1e-24)
	~	
	~		|

Quanta variation rate=
	-efficiency*(Quanta/Ventilation Cycle time)
	~	quanta/min
	~		|

I=
	(I1*(H)*(H))/("d(t)"*"d(t)")
	~	W/(cm*cm)
	~		|

"d(t)"=
	Max(0.01,((H*100)/te)*(-1)*(Time+error)+(H*100)+0.01)
	~	cm
	~		|

efficiency=
	(1-"N(t)"/N0)
	~	
	~		|

"N(t)"= INTEG (
	-"dN(t)/dt",
		N0)
	~	Dmnl
	~		|

"dN(t)/dt"=
	Max(IF THEN ELSE( Time<te , ("N(t)"-Nref)/"AT(t)" , 0 ),0)
	~	1/s
	~		|

error=
	1e-20
	~	
	~		|

I1=
	2
	~	W/(cm*cm)
	~		|

Nref=
	0
	~	Dmnl
	~		|

k=
	1870
	~	(cm*cm)/(W*s)
	~	1.87 cm2/(mW.s)
	|

N0=
	1
	~	Dmnl
	~		|

te=
	(3.14159/4)*((H*D*D)/Ventilator Flow)*60
	~	sec
	~	Exposure time
	|

Initial Quanta=
	100
	~	quanta
	~		|

Quanta= INTEG (
	Quanta variation rate,
		Initial Quanta)
	~	quanta
	~		|

Room Volume=
	665
	~	m*m*m
	~		|

Ventilation Cycle time=
	(Room Volume/Ventilator Flow)*60
	~	min
	~		|

D=
	3
	~	m
	~	Ventilator Diameter
	|

Ventilator Flow=
	350
	~	m*m*m/min
	~		|

H=
	2.41
	~	m
	~	Ventilator Head
	|

